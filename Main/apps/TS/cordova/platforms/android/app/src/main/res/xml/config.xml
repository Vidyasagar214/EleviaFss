<?xml version='1.0' encoding='utf-8'?>
<widget id="com.mobile.FSS" version="4.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>FSS</name>
    <description>
        EleVia Software Field Services Suite
    </description>
    <author email="support@eleviasoftware.com" href="http://eleviasoftware.com">
        EleVia Software Development Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="mdpi" src="../resources/images/FSS_Main_Native.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
